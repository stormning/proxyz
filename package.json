{
  "name": "proxyz",
  "version": "1.0.0",
  "main": "main.js",
  "author": "stormning",
  "scripts": {
    "watch": "webpack-dev-server --devtool eval --progress --colors",
    "pack": "build --dir",
    "dist": "build",
    "dev": "electron .",
    "clean": "rm -rf ./dist",
    "clean:mac": "rm -rf ./dist/mac",
    "pack:mac": "npm run clean:mac && electron-packager . \"proxyz\" --out=dist/mac --platform=darwin --arch=x64 --version=0.36.2 --icon=assets/mac/logo.icns --ignore=dist --ignore=assets --ignore=builder.json --ignore=bower.json --ignore=README.md --ignore=.gitignore --ignore=preview.png"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.2",
    "electron": "^1.6.7",
    "electron-builder": "^17.8.0",
    "electron-packager": "^8.7.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "html-webpack-plugin": "^2.28.0",
    "prop-types": "^15.5.10",
    "style-loader": "^0.18.0",
    "webpack": "^2.6.0",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "material-ui": "^0.18.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-router-dom": "^4.1.1",
    "react-tap-event-plugin": "^2.0.1"
  }
}
